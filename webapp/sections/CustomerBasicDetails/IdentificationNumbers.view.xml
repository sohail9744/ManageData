<mvc:View
    controllerName="Iffco.clap.sections.CustomerBasicDetails.IdentificationNumbersController"
    xmlns:u="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:forms="sap.ui.layout.form"
    xmlns="sap.m"
>
    <forms:SimpleForm
        editable="false"
        layout="ColumnLayout"
    >
        <Label
            text="Identification Type"
            visible="{=${fieldMappingModels>/IdentificationTypeVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/IdentificationTypeMandatory} === true ? true : false}"
        />
        <Input
            enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}"
            value="{Customers>/zidentification_type}"
            maxLength="6"
            visible="{=${fieldMappingModels>/IdentificationTypeVisible} === true ? true : false}"
        />
        <Label
            text="Identification Number"
            visible="{=${fieldMappingModels>/IdentificationNumberVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/IdentificationNumberMandatory} === true ? true : false}"
        />
        <Input
            enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}"
            value="{Customers>/zidentification_number}"
            maxLength="60"
            visible="{=${fieldMappingModels>/IdentificationNumberVisible} === true ? true : false}"
        />

        <Label
            text="VAT/GST Number"
            visible="{=${fieldMappingModels>/VAT-GSTNoVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/VAT-GSTNoVisibleMandatory} === true ? true : false}"
        />
        <Input
            enabled="{appView>/clapFields}"
            value="{Customers>/zvat_gst_number}"
            liveChange="handleSetMaxLength"
            type="Number"
            maxLength="60"
            visible="{=${fieldMappingModels>/VAT-GSTNoVisible} === true ? true : false}"
        />

        <Label
            text="Attachment (VAT /GST No Attachment)"
            visible="{=${fieldMappingModels>/VAT-GSTNoVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/VAT-GSTNoVisibleMandatory} === true ? true : false}"
        />
        <u:FileUploader
            enabled="{appView>/clapFields}"
            value="{getDmsModel>/vatAttachment}"
            visible="{=${fieldMappingModels>/VAT-GSTNoVisible} === true ? true : false}"
            id="vatAttachment"
            name="fileUploader"
            buttonText="Upload File"
            fileType="pdf"
            sendXHR="true"
            change="onConfirm"
        />
        <!-- <Input value="{Customers>/zvat_gstno_attachment}" visible="{=${fieldMappingModels>/VAT-GSTNoVisible} === true ? true : false}"/> -->

        <Label
            text="License Type"
            visible="{=${fieldMappingModels>/LicenseTypeVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/LicenseTypeMandatory} === true ? true : false}"
        />
        <!-- <Input enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}" value="{Customers>/zlicense_type}" visible="{=${fieldMappingModels>/LicenseTypeVisible} === true ? true : false}"/> -->
        <MultiInput
            showClearIcon="true"
            value="{Customers>/zlicense_type}"
            enabled="{appView>/clapFields}"
            valueHelpRequest="handleValueHelpForLicense"
            visible="{=${fieldMappingModels>/LicenseTypeVisible} === true ? true : false}"
            valueHelpOnly="true"
        />

        <Label
            text="License Number"
            visible="{=${fieldMappingModels>/LicenseNoVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/LicenseNoMandatory} === true ? true : false}"
        />
        <Input
            enabled="{appView>/clapFields}"
            value="{Customers>/zlicense_no}"
            maxLength="60"
            visible="{=${fieldMappingModels>/LicenseNoVisible} === true ? true : false}"
        />

        <Label
            text="License Attachment"
            visible="{=${fieldMappingModels>/LicenseAttachmentmandatoryVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/LicenseAttachmentmandatory} === true ? true : false}"
        />
        <u:FileUploader
            enabled="{appView>/clapFields}"
            value="{getDmsModel>/licenseAttachment}"
            visible="{=${fieldMappingModels>/LicenseAttachmentmandatoryVisible} === true ? true : false}"
            id="licenseAttachment"
            name="fileUploader"
            buttonText="Upload File"
            fileType="pdf"
            sendXHR="true"
            change="onConfirm"
        />

        <Label
            text="Valid from"
            visible="{=${fieldMappingModels>/ValidFromVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/ValidFromMandatory} === true ? true : false}"
        />
        <DatePicker
            enabled="{appView>/clapFields}"
            id="validFrom"
            value="{Customers>/zvalid_from}"
            visible="{=${fieldMappingModels>/ValidFromVisible} === true ? true : false}"
            valueFormat="MM.dd.yyyy"
            change="handleStartDateChange"
            displayFormat="long"
        />

        <Label
            text="Valid to"
            visible="{=${fieldMappingModels>/ValidToVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/ValidToMandatory} === true ? true : false}"
        />
        <DatePicker
            enabled="{appView>/clapFields}"
            id="validTo"
            value="{Customers>/zvalid_to}"
            visible="{=${fieldMappingModels>/ValidToVisible} === true ? true : false}"
            valueFormat="MM.dd.yyyy"
            change="handleEndDateChange"
            displayFormat="long"
        />

        <Label
            text="Import License Number"
            visible="{=${fieldMappingModels>/ImportLicenseNumberVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/ImportLicenseNumberMandatory} === true ? true : false}"
        />
        <Input
            enabled="{appView>/clapFields}"
            value="{Customers>/zimport_license_number}"
            visible="{=${fieldMappingModels>/ImportLicenseNumberVisible} === true ? true : false}"
        />

        <Label
            text="Attachment (Import License Number)"
            visible="{=${fieldMappingModels>/ImportLicenseNumberVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/ImportLicenseNumberMandatory} === true ? true : false}"
        />
        <u:FileUploader
            enabled="{appView>/clapFields}"
            value="{getDmsModel>/importLicenseNumberAtachmnt}"
            visible="{=${fieldMappingModels>/ImportLicenseNumberVisible} === true ? true : false}"
            id="importLicenseNumberAtachmnt"
            name="fileUploader"
            buttonText="Upload File"
            fileType="pdf"
            sendXHR="true"
            change="onConfirm"
        />
        <!-- <Input value="{Customers>/zimport_license_number_atachmnt}" visible="{=${fieldMappingModels>/ImportLicenseNumberVisible} === true ? true : false}"/> -->
        <Label
            text="Source of Enquiry"
            visible="{=${fieldMappingModels>/SourceofEnquiryVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/SourceofEnquiryMandatory} === true ? true : false}"
        />
        <!-- <Input enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}" value="{Customers>/zsource_of_inquiry}" visible="{=${fieldMappingModels>/SourceofEnquiryVisible} === true ? true : false}"/> -->
        <MultiInput
            showClearIcon="true"
            value="{Customers>/zsource_of_inquiry}"
            enabled="{appView>/clapFields}"
            valueHelpRequest="handleValueHelpForSourceOfEnquiry"
            visible="{=${fieldMappingModels>/SourceofEnquiryVisible} === true ? true : false}"
            valueHelpOnly="true"
        />
        <Label
            text="Responsible Institute"
            visible="{=${fieldMappingModels>/ResonsibleInstituteVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/ResonsibleInstituteMandatory} === true ? true : false}"
        />
        <Input
            enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}"
            value="{Customers>/zresonsible_institute}"
            visible="{=${fieldMappingModels>/ResonsibleInstituteVisible} === true ? true : false}"
        />
        <Label
            text="Entry Date"
            visible="{=${fieldMappingModels>/EntryDateVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/EntryDateMandatory} === true ? true : false}"
        />
        <DatePicker
            enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}"
            value="{Customers>/zentry_date}"
            visible="{=${fieldMappingModels>/EntryDateVisible} === true ? true : false}"
            valueFormat="MM.dd.yyyy"
            change="handleLiveChange"
            displayFormat="long"
        />

        <Label
            text="Country"
            visible="{=${fieldMappingModels>/CountryVisibleIdentificationNumbers} === true ? true : false}"
            required="{=${fieldMappingModels>/CountryMandatoryIdentificationNumbers} === true ? true : false}"
        />
        <!-- <Input enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}" value="{Customers>/zcountry}" visible="{=${fieldMappingModels>/CountryVisible} === true ? true : false}"/> -->
        <Input
            enabled="false"
            value="{Customers>/zcountry}"
            visible="{=${fieldMappingModels>/CountryVisibleIdentificationNumbers} === true ? true : false}"
        />
        <Label
            text="Region"
            visible="{=${fieldMappingModels>/RegionVisible} === true ? true : false}"
            required="{=${fieldMappingModels>/RegionMandatory} === true ? true : false}"
        />
        <!-- <Input enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}" value="{Customers>/zregion}" visible="{=${fieldMappingModels>/RegionVisible} === true ? true : false}"/> -->
        <Input
            enabled="{=${Customers>/zrequest_status} !== 'In Progress' ? true : false}"
            value="{Customers>/zregion}"
            visible="{=${fieldMappingModels>/RegionVisible} === true ? true : false}"
        />
    </forms:SimpleForm>
</mvc:View>
