<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:forms="sap.ui.layout.form" xmlns="sap.m" >
	<forms:SimpleForm editable="false" layout="ColumnLayout">
		<Label text="Address Version" visible="{=${fieldMappingModel>/20/visibility} === 'Y' ? true : false}"
 required="{=${fieldMappingModel>/20/mandatory} === 'Y' ? true : false}"/>
		<Input value="{Customers>/zaddress_version}" visible="{=${fieldMappingModel>/20/visibility} === 'Y' ? true : false}"/>
		<Label text = "Title" visible="{=${fieldMappingModel>/427/visibility} === 'Y' ? true : false}"
 required="{=${fieldMappingModel>/427/mandatory} === 'Y' ? true : false}"/>
		<Input value="{Customers>/ztitle}" visible="{=${fieldMappingModel>/427/visibility} === 'Y' ? true : false}"/>
		
		<Label text="Name" visible="{=${fieldMappingModel>/288/visibility} === 'Y' ? true : false}"
 required="{=${fieldMappingModel>/288/mandatory} === 'Y' ? true : false}"/>
		<Input value="{Customers>/zfirst_name}" visible="{=${fieldMappingModel>/288/visibility} === 'Y' ? true : false}"/>
		<Label text="Name 2" visible="{=${fieldMappingModel>/289/visibility} === 'Y' ? true : false}"
 required="{=${fieldMappingModel>/289/mandatory} === 'Y' ? true : false}"/>
		<Input value="{Customers>/zlast_name}" visible="{=${fieldMappingModel>/289/visibility} === 'Y' ? true : false}"/>
		<Label text= "Search term" visible="{=${fieldMappingModel>/394/visibility} === 'Y' ? true : false}"
 required="{=${fieldMappingModel>/394/mandatory} === 'Y' ? true : false}"/>
		<Input value="{Customers>/zsearch_term}" visible="{=${fieldMappingModel>/394/visibility} === 'Y' ? true : false}"/>
		<!-- <Label text="Street House Number" visible="{=${fieldMappingModel>/211/visibility} === 'Y' ? true : false}"
 required="{=${fieldMappingModel>/211/mandatory} === 'Y' ? true : false}"/>
		<Input value="{Customers>/zstreet_house_number}" visible="{=${fieldMappingModel>/211/visibility} === 'Y' ? true : false}"/> -->
        <Label text="City" visible="{=${fieldMappingModel>/94/visibility} === 'Y' ? true : false}"
 required="{=${fieldMappingModel>/94/mandatory} === 'Y' ? true : false}"/>
		<Input value="{Customers>/zcity}" visible="{=${fieldMappingModel>/94/visibility} === 'Y' ? true : false}"/>
		
	</forms:SimpleForm>
</mvc:View>