<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        contentHeight="60%"
        showClearButton="true"
        title="Delivery Block Selected"
        class="sapUiPopupWithPadding"
        items="{S4D>/DeliveryBlockSet}"
        search="handleValueHelpSalesDelBlockSel"
        confirm="handleDelBlockSelConfirm"
        cancel=""
        multiSelect="false"
    >
        <StandardListItem
            title="{S4D>Deliveryblock}"
            description="{S4D>DeliveryblockDesc}"
        />
    </SelectDialog>
</core:FragmentDefinition>

