<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core">
    <SelectDialog
        contentHeight="60%"
        showClearButton="true"
        title="Sales Order Block selected"
        class="sapUiPopupWithPadding"
        items="{path:'S4D>/SalesBlockSet'}"
        search="handleSelBlockSalesSearch"
        confirm="handleSelBlockSalesConfirm"
        cancel=""
        multiSelect="false"
    >
        <StandardListItem
            title="{S4D>Salesorderblock}"
            description="{S4D>SalesorderblockDesc}"
        />
    </SelectDialog>
</c:FragmentDefinition>
